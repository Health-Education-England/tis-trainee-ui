{
  "name": "trainee-ui-app",
  "version": "0.24.5",
  "private": true,
  "dependencies": {
    "@sentry/browser": "^6.3.4",
    "@types/node": "^14.14.32",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/yup": "0.29.11",
    "aws-amplify": "^3.3.13",
    "aws-amplify-react": "^4.2.30",
    "axios": "^0.21.1",
    "browser-update": "^3.3.25",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-prettier": "^3.4.0",
    "formik": "^2.2.6",
    "moment": "^2.29.1",
    "nhsuk-frontend": "^3.1.0",
    "nhsuk-react-components": "^1.2.5",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-cookie-consent": "^5.2.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-loading": "^2.0.3",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^16.13.0",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "sass": "^1.32.13",
    "stylelint": "^13.12.0",
    "typescript": "^4.2.4",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "npm run generate-build-version",
    "generate-build-version": "node generate-build-version",
    "build": "react-scripts build",
    "test": "react-scripts test --silent --watchAll=false --coverage --testResultsProcessor jest-sonar-reporter",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx,.js --fix",
    "stylelint": "stylelint \"src/**/*.scss\" \"src/**/*.css\" --fix",
    "report:merge": "mochawesome-merge cypress/results/json/*.json > cypress/results/index.json",
    "report:generate": "marge cypress/results/index.json --reportDir cypress/reports --assetsDir cypress/reports/assets --reportPageTitle index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run stylelint"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.23",
    "@types/react-test-renderer": "^16.9.3",
    "@types/redux-mock-store": "^1.0.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "cypress": "^6.8.0",
    "cypress-multi-reporters": "^1.4.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.11.0",
    "husky": "^5.1.3",
    "jest-sonar-reporter": "^2.0.0",
    "mocha": "^8.2.1",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "stylelint-prettier": "^1.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/src/mock-data/*.{js,jsx,ts,tsx}",
      "!**/src/redux/store/*.{js,jsx,ts,tsx}",
      "!**/src/redux/types.ts",
      "!**/src/components/forms/formr-part-b/Sections/SectionProps.ts",
      "!**/src/*.{js,ts}",
      "!**/src/index.tsx",
      "!**/src/models/*.ts",
      "!**/src/redux/reducers/index.ts",
      "**/src/**/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "json",
      "html",
      "lcov",
      "text"
    ],
    "resetMocks": false
  },
  "jestSonar": {
    "sonar56x": true,
    "reportPath": ".",
    "reportFile": "coverage/sonar-report.xml",
    "indent": 4
  }
}
