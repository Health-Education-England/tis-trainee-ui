{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "exclude": [
    "**/build/*",
    "**/coverage/*",
    "**/cypress/*",
    "**/cypress-coverage/*",
    "**/*.d.ts",
    "**/generate-build-version.{js,ts}",
    "**/jest-coverage/*",
    "**/List.tsx",
    "**/*mergeCoverage.js",
    "**/*serviceWorker.ts",
    "**/*setupTests.ts",
    "**/*.spec.{js,ts,tsx}",
    "**/*store.ts",
    "**/*.test.{js,ts,tsx}",
    "**/__test__/*",
    "**/types.ts"
  ],
  "report-dir": "cypress-coverage"
}
