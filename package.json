  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "prebuild": "npm run generate-build-version",
    "generate-build-version": "node generate-build-version",
    "build": "react-scripts build",
    "test": "react-scripts test --silent --watchAll=false --coverage --coverageDirectory=jest-coverage",
    "test-cypress": "npm run cypress & npm run ct & npm run coverage:nyc",
    "coverage:nyc": "npx nyc report --reporter=text",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "cypress:debug": "cross-env-shell DEBUG=code-coverage npx cypress open",
    "ct:open": "npx cypress open-ct",
    "ct": "npx cypress run-ct",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx,.js --fix",
    "stylelint": "stylelint \"src/**/*.scss\" \"src/**/*.css\" --fix",
    "report:merge": "mochawesome-merge cypress/results/json/*.json > cypress/results/index.json",
    "report:generate": "marge cypress/results/index.json --reportDir cypress/reports --assetsDir cypress/reports/assets --reportPageTitle index.html",
    "report:combined": "node ./mergeCoverage.js",
    "local:report-combined": "npm test & npm run ct & npm run report:combined"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run stylelint"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.23",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-mock-store": "^1.0.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "cypress-multi-reporters": "^1.4.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.30.0",
    "husky": "^6.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "mocha": "^9.0.2",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "stylelint-prettier": "^1.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.{ts,tsx}",
      "!**/src/mock-data/*.{js,jsx,ts,tsx}",
      "!**/src/redux/store/*.{js,jsx,ts,tsx}",
      "!**/src/redux/types.ts",
      "!**/src/components/forms/formr-part-b/Sections/SectionProps.ts",
      "!**/src/*.{js,ts}",
      "!**/src/index.tsx",
      "!**/src/models/*.ts",
      "!**/src/redux/reducers/index.ts",
      "!**/src/**cy-test.tsx"
    ],
    "coverageReporters": [
      "json",
      "html",
      "lcov",
      "text"
    ],
    "resetMocks": false
  }
}
