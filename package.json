{
  "name": "trainee-ui-app",
  "version": "0.55.7",
  "private": true,
  "dependencies": {
    "@aws-amplify/ui-react": "^3.6.0",
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/instrument-cra": "^1.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^1.9.0",
    "@sentry/browser": "^7.21.1",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.11.09",
    "@types/qrcode.react": "^1.0.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-gtm-module": "^2.0.1",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^17.0.1",
    "aws-amplify": "^4.3.43",
    "axios": "^1.2.0",
    "browser-update": "^3.3.44",
    "cypress": "^11.2.0",
    "cypress-otp": "^1.0.3",
    "dayjs": "^1.10.7",
    "enzyme": "^3.11.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "formik": "^2.2.9",
    "husky": "^8.0.2",
    "jest-sonar-reporter": "^2.0.0",
    "material-ui-confirm": "^3.0.7",
    "mocha": "^10.1.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "nanoid": "^4.0.0",
    "nhsuk-frontend": "^6.1.2",
    "nhsuk-react-components": "^2.2.0",
    "prettier": "^2.8.0",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-gtm-module": "^2.0.11",
    "react-helmet": "^6.1.0",
    "react-loading": "^2.0.3",
    "react-phone-number-input": "^3.2.12",
    "react-redux": "^8.0.5",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.0",
    "react-select": "^5.6.1",
    "react-test-renderer": "^18.2.0",
    "redux": "^4.2.0",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "sass": "^1.32.13",
    "stylelint": "^14.15.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.6.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "npm run generate-build-version",
    "generate-build-version": "node generate-build-version",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!@amcharts)/\" --silent --watchAll=false --coverage --coverageDirectory=jest-coverage",
    "test-cypress": "npm run cypress & npm run ct & npm run coverage:nyc",
    "coverage:nyc": "npx nyc report --reporter=text",
    "cypress": "cypress run --e2e",
    "cypress:open": "cypress open --e2e",
    "cypress:debug": "cross-env-shell DEBUG=code-coverage npx cypress open",
    "ct:open": "cypress open --component",
    "ct": "cypress run --component",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx,.js --fix",
    "stylelint": "stylelint \"src/**/*.scss\" \"src/**/*.css\" --fix",
    "report:merge": "mochawesome-merge cypress/results/json/*.json > cypress/results/index.json",
    "report:generate": "marge cypress/results/index.json --reportDir cypress/reports --assetsDir cypress/reports/assets --reportPageTitle index.html",
    "report:combined": "node ./mergeCoverage.js",
    "local:report-combined": "npm test & npm run ct & npm run report:combined"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run stylelint"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.{ts,tsx}",
      "!**/src/mock-data/*.{js,jsx,ts,tsx}",
      "!**/src/redux/store/*.{js,jsx,ts,tsx}",
      "!**/src/redux/types.ts",
      "!**/src/*.{js,ts}",
      "!**/src/index.tsx",
      "!**/src/models/*.ts",
      "!**/src/redux/reducers/index.ts"
    ],
    "coverageReporters": [
      "json",
      "html",
      "lcov",
      "text"
    ],
    "resetMocks": false
  }
}
